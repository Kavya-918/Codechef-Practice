import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	   Scanner sc = new Scanner(System.in);
	   int x = sc.nextInt();
	   if(x==17)
	   {
	       System.out.println("YAY");
	   }
	   else
	   {
	       System.out.println("NO");
	   }

	}
}
